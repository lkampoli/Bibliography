@article{pub.1041375944,
 abstract = {A mass-matrix formulation of the fluctuation splitting schemes for solving compressible, unsteady flows is proposed. This formulation is consistent with the conservative linearisation based on parameter vector and allows to extend to unsteady flows the ‘invariance under similarity transformations’ property that had been shown to hold for the steady version of the schemes. Second-order time accuracy is achieved using a Petrov–Galerkin finite element interpretation of the fluctuation splitting schemes. The approach may however be readily applicable to all other time-accurate fluctuation splitting formulations that have been so far proposed in the literature. Applications of the proposed methodology to two- and three-dimensional, inviscid and viscous compressible flows are reported and discussed in the paper.},
 author = {Bonfiglioli, Aldo and Paciorri, Renato},
 doi = {10.1080/10618562.2013.813491},
 journal = {International Journal of Computational Fluid Dynamics},
 keywords = {},
 number = {4-5},
