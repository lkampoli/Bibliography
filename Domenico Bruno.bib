 title = {Direct simulation Monte Carlo simulation of thermal fluctuations in gases},
 url = {https://app.dimensions.ai/details/publication/pub.1113625766},
 volume = {31},
 year = {2019}
}

@article{pub.1117914419,
 abstract = {Rayleigh-Brillouin scattering spectra of SF6 are modeled at room temperature in the pressure range [0.2–5] bar and compared to recent experimental measurements (Wang et al., 2017). It is shown that lineshape models that account for density-dependent corrections to the thermodynamic properties provide substantially better agreement than the models assuming an ideal gas model. For intermediate pressures, where a hydrodynamic description of the spectra fails due to kinetic effects, the Enskog-Vlasov kinetic model is shown to reproduce experimental data with good accuracy both at low and high pressure.},
 author = {Bruno, D. and Frezzotti, A.},
 doi = {10.1016/j.cplett.2019.136595},
