 pages = {905840308},
 title = {Molecular dynamics calculation of the spectral densities of plasma fluctuations},
 url = {https://app.dimensions.ai/details/publication/pub.1104394720},
 volume = {84},
 year = {2018}
}

@article{pub.1113625766,
 abstract = {Work done by the authors on the Direct Simulation Monte Carlo (DSMC) simulation of thermal fluctuation in gases is summarized here. The calculation of the gas transport properties via the Green-Kubo formulas is discussed. Results from classical trajectory DSMC simulations of molecular oxygen show how the approach can be used to validate a particular interaction model (a Potential Energy Surface in this case). Direct experimental validation of the dynamics of spontaneous density fluctuations is also possible due to its connection to the spectrum measured in Rayleigh-Brillouin light scattering experiments (RBS). A number of examples of the DSMC simulation of RBS spectra for atomic gases and their mixtures, and for a molecular gas (oxygen) are discussed. Finally, an extension of the method is discussed that allows discussion of small density-dependent nonideality effects in the RBS spectra of SF6.},
