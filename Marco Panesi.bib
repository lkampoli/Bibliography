@article{pub.1033090578,
 abstract = {A review is made of the computational models of high-enthalpy flows developed over the past few years at the von Karman Institute and Université Libre de Bruxelles, for the modelling of high-enthalpy hypersonic (re-)entry flows. Both flows in local thermo-chemical equilibrium (LTE) and flows in thermo-chemical non-equilibrium (TCNEQ) are considered. First, the physico-chemical models are described, i.e. the set of conservation laws, the thermodynamics, transport phenomena and chemical kinetics models. Particular attention is given to the correct modelling of elemental (LTE flows) and species (chemical non-equilibrium—CNEQ—flows) transport. The numerical algorithm, based on a state-of-the-art finite volume discretization, is then briefly described. Finally, selected examples are included to illustrate the capabilities of the developed solver.},
 author = {Degrez, G and Lani, A and Panesi, M and Chazot, O and Deconinck, H},
