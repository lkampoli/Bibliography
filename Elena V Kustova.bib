}

@inproceedings{pub.1098584237,
 abstract = {Chemically nonequilibrium ionized gas mixture flows of (N, N+, e−) and (O, O+, e−) with electronic excitation of neutral component are studied. The one-temperature model of transport properties taking into account electronic states of neutral atoms is developed. The transport coefficients of thermal and electrical conductivity, diffusion and thermal diffusion, shear and bulk viscosity are calculated in the temperature range 500-50,000 K for various mixture compositions. Contribution of the electronic degrees of freedom to the transport properties is found to be important.},
