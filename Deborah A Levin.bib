 url = {https://app.dimensions.ai/details/publication/pub.1139485278},
 year = {2003}
}

@inproceedings{pub.1139486055,
 abstract = {A general model to calculate thermally and chemically inelastic collisions in the direct simulation Monte Carlo method is discussed. The advantages of using discrete internal energy distributions are summarized. The principal aspects of the model, such as the selection methodology for inelastic collision events, and matching internal energy transfer and chemical reaction rates to the experimental data, are examined. The correction procedure to maintain accurate rates for the vibration‐translation energy transfer and chemical reactions with discrete internal energies are given.},
 author = {Gimelshein, N. E. and Gimelshein, S. F. and Levin, D. A. and Ivanov, M. S. and Wysong, I. J.},
