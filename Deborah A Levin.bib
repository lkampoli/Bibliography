 number = {11},
 pages = {112105},
 title = {Kinetic modeling and experiments of a pulsed-bias plasma in a multipole plasma chamber},
 url = {https://app.dimensions.ai/details/publication/pub.1152548956},
 volume = {29},
 year = {2022}
}

