}

@inproceedings{pub.1139465002,
 abstract = {The modeling of micro flows through a 3D micro nozzle was investigated by means of the DSMC and NS techniques in order to verify the influence of the computational scheme parameters on the predicted flow quantities. The investigated low Reynold’s number flow has a complex structure containing subsonic and supersonic parts. Since the influence of the boundary layer on the flow characteristics is significant, a strong effort was made to predict the boundary layer structure as exactly as possible.},
 author = {Titov, E. V. and Levin, D. A.},
 booktitle = {AIP Conference Proceedings},
 doi = {10.1063/1.1941627},
 keywords = {},
