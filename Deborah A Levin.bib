@inproceedings{pub.1123825840,
 abstract = {Particle-laden electrostatic discharges are widely used in experimental research as well as industries. The mechanism of particle lifting in these highly unsteady flows are not well understood. Modifications are made to the FLASH research code to simulate this multiphase system using the Lagrangian-Eulerian (LE) approach. Various Lagrangian submodels are implemented in the code to study the particle phase evolution in the system. Results generated from the newly developed code are compared to the experimental results for the dispersion of glass beads in an electrostatic discharge of energy 2 J performed at the New Jersey Institute of Technology (NJIT). The simulation results are qualitatively in good agreement with the experimental results. Furthermore, the prominent mechanism of particle lifting was identified to be the Saffman lifting force generated on the particles due to velocity gradients in the gas flow field. Results suggest that the newly developed code is flexible in modeling particle transportation on larger length scales such as flow generated by fireball plasmas.},
 author = {Marayikkottu Vijayan, Akhil and Sawant, Saurabh S. and Levin, Deborah A. and Huang, Ci and Schoenitz, Mirko and Dreizin, Edward},
 booktitle = {AIAA Scitech 2020 Forum},
 doi = {10.2514/6.2020-1798},
 keywords = {},
