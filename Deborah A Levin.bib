@article{pub.1148860606,
 abstract = {Experiments were conducted at the University of Illinois at Urbana Champaign Electric Propulsion Laboratory to study the ion-induced secondary charge emission from surfaces bombarded by an [Emim][BF4] electrospray plume. The surfaces were unpolished and had oxide and hydrocarbon layers typical of as-received materials used in electrospray experiments. The effect of changing the bias from −85 to +85 V on the target collecting current from the electrospray plume was measured. These data are used to calculate the yield of positive and negative charged species emission from the target due to bombarding cations and anions. The yields of positive charges per incident cation (γcation+), negative charges per incident cation (γcation−), positive charges per incident anion (γanion+), and negative charges per incident anion (γanion−) are measured for eight different industrial materials commonly used in electrospray diagnostics or thruster and spacecraft construction. These yields range from 0 to 1.3 charges per incident ion over a range of emitter voltages from 1.5 to 2.9 kV and largely display linearly increasing trends with increasing emitter voltage. Over this emitter voltage range, γcation+ ranges from 0 to 0.55, γcation− ranges from 0 to 0.75, γanion+ ranges from 0 to 0.4, and γanion− ranges from 0.3 to 1.3. The data show for the first time positive and negative charges being emitted due to both cation and anion bombardment and suggest that secondary ions sputtered from the surface are important to the secondary charge emission behavior in electrospray plumes.},
 author = {Klosterman, Matthew R. and Rovey, Joshua L. and Levin, Deborah A.},
 doi = {10.1063/5.0060615},
 journal = {Journal of Applied Physics},
 keywords = {},
 number = {24},
 pages = {243302},
 title = {Ion-induced charge emission from unpolished surfaces bombarded by an [Emim][BF4] electrospray plume},
 url = {https://app.dimensions.ai/details/publication/pub.1148860606},
 volume = {131},
 year = {2022}
}

@inproceedings{pub.1149276482,
 abstract = {The present study investigates the thermionically emitting surfaces immersed in flowing plasmas by using an electrostatic Particle-in-cell (PIC) code, CHAOS [1], which is an efficient GPU-based solver developed at the University of Illinois Urbana-Champaign. The nature of emissive sheaths submersed in low density (~1014 m-3) and high density (~1016 m-3) plasmas is examined by performing 1-D collisionless simulations. In this work, we will show that, in both cases, there is a virtual cathode formation near the emitting surface where an electron cloud accumulates and a space-charge-limiting (SCL) sheath forms. The SCL sheath is about 10 Debye lengths in thickness with a 16 Debye lengths thick quasi-neutral, and 8 Debye lengths thick inlet-sheath region. Since the electrons are emitted with a surface temperature of 0.17 eV, a voltage drop of the same order is found for the virtual cathode. We validate our PIC simulations by comparing the SCL currents with the estimates from Child-Langmuir Law [2] and an improved sheath theory developed by Hanquist et al [3]. We plan to show how variables of emissive surface temperature and plasma density affect the SCL current with application to electron-transpiration-cooling over hypersonic surfaces.},
 author = {Vatansever, D. and Nuwal, N. and Levin, D. A.},
 booktitle = {2022 IEEE International Conference on Plasma Science (ICOPS)},
 doi = {10.1109/icops45751.2022.9813183},
 keywords = {},
 pages = {1-2},
 title = {Modeling of Electron Emitting Surfaces Submersed in Boundary Layer Plasmas},
 url = {https://app.dimensions.ai/details/publication/pub.1149276482},
 year = {2022}
}

@inproceedings{pub.1149276616,
 abstract = {Ion beams are used in various engineering applications such as particle accelerators, ion-thrusters, and ion-implantations. The electrons are introduced to neutralize the beam and reduce their divergence along the axis. This is most economically achieved by introducing electrons via an external filament source. However, in recent numerical works by Lan et al.[1], [2], electrostatic-solitary-waves (ESWs) were observed for such a setup in a 2D ion beam where the ESWs slowed the beam neutralization process [2]. We present a Particle-in-Cell [3] study of the beam neutralization in both 2D and 3D beams and show the formation of ESWs and their movement along the beam axis. Further, by performing theoretical analysis, we will show the uniqueness of these ESWs found in ion beams where a non-Maxwellian electron distribution allows for ESWs of large lengths to form. We will also present our findings on the long-wavelength cylindrical surface waves [4] found in our 3D simulations and report the change in their properties with the beam radii. The prediction of surface waves and their frequencies is vital in experimental diagnostics and passive measurements in plasma beams [5].},
 author = {Nuwal, N. and Levin, D. A. and Kaganovich, I. D.},
 booktitle = {2022 IEEE International Conference on Plasma Science (ICOPS)},
 doi = {10.1109/icops45751.2022.9813317},
 keywords = {},
 pages = {1-2},
 title = {Kinetic Modeling of Solitary and Surface Waves in 3D Beam Neutralization},
 url = {https://app.dimensions.ai/details/publication/pub.1149276616},
 year = {2022}
}

@article{pub.1150413208,
 abstract = {Hypersonic flow over a cylinder was modeled using the direct simulation Monte Carlo method to study how nitric oxide (NO) Ultraviolet emission profiles are a test of high-fidelity thermochemical, nonequilibrium models. For pressures found in typical hypersonic ground facilities, it was shown that the ultraviolet radiation emission profiles are very close to those calculated by assuming Boltzmann equilibrium conditions. Therefore, the NO emission profiles can be tied directly to the ground state NO concentration and flow bulk temperature. Two chemical models differing in the manner in which molecular oxygen is dissociated were considered in this classic canonical-type flow. A comparison of the predicted flowfields with schlieren imagery and surface pressure measurements from previous experiments showed good agreement; however, the sensitivity of these measurements to change in different freestream species concentrations was not found to be strong. Instead, the shapes of the predicted NO integrated emission spatial profiles were observed to be highly sensitive to whether vibrational favoring was considered in the oxygen dissociation model for the same freestream conditions.},
 author = {Karpuzcu, Irmak T. and Jouffray, Matthew P. and Levin, Deborah A.},
 doi = {10.2514/1.t6609},
 journal = {Journal of Thermophysics and Heat Transfer},
 keywords = {},
 number = {1},
 pages = {147-160},
 title = {Effect of Oxygen Dissociation on Nitric Oxide Ultraviolet Emissions},
 url = {https://app.dimensions.ai/details/publication/pub.1150413208},
 volume = {37},
 year = {2023}
}

