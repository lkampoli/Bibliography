@article{pub.1038435734,
 abstract = {A chemical flow reactor is used to study the vibrational population distribution of CO produced by a reaction between carbon vapor generated in an arc discharge and molecular oxygen. The results demonstrate formation of highly vibrationally excited CO, up to vibrational level v=14, at low temperatures, T=400–450K, with population inversion at v=4–7, in a collision-dominated environment, 15–20Torr. The average vibrational energy per CO molecule formed by the reaction is 0.6–1.2eV/molecule, which corresponds to 10–20% of reaction enthalpy. The results show feasibility of development of a new CO chemical laser using carbon vapor and oxygen as reactants.},
