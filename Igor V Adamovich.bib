@inproceedings{pub.1149276572,
 abstract = {Atmospheric pressure plasmas (APPs) are used for treating surfaces from polymers to living tissue which are geometrically complex and non-planar. Dielectric surfaces support propagation of surface ionization waves (SIWs) which charge the surface with there being electric field enhancement due to curvature of the surface. These conditions produce a nonlinear feedback to the SIW which is the source of the charging. In this paper, we discuss results from computational and experimental investigations of SIW propagation over a series of 250 µm × 250 µm dry and water-filled channels. The SIW results from a pulsed APP jet sustained in Ar/N2=90/10 with a shielding co-flow of N2 into room air, incident on a machined macor tube.},
 author = {Konina, K. and Raskar, S. and Adamovich, I. and Kushner, M. J.},
