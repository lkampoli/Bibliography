}

@article{pub.1030181641,
 abstract = {In this paper, a high-order Navier-Stokes solver is developed for chemically reacting flow and is applied to investigate the unsteady, viscous interaction between shock wave and boundary layer in an air filled shock tube. The solver employs a high- resolution weighted essentially non-oscillatory (WENO) scheme to capture the complex flow pattern induced by shock/boundary- layer interaction, together with a point-implicit method to overcome the stiffness of chemical production term. The results show the influence of real gas effects and give insights into the mechanism of shock/boundary-layer interaction process.},
