@article{pub.1155428824,
 abstract = {Electron-impact cross sections for the v→v′ transitions between the vibrational levels of X1Σ+ and the excited B1Π electronic states of LiH molecule, are calculated from threshold to 1000 eV by using the threshold-modified Mott–Massey approximation. Cross sections for the dissociative process, occurring through the excitation to the continuum spectrum of the vibrational levels of the B state, are also obtained. Maxwellian rate coefficients for the corresponding bound−to−bound and bound−to−continuum transitions are presented in the temperature range of 0.1–10 000 eV. Finally, scaling relationships for both the v−v′ cross section and rate coefficients are derived in order to represent the calculated quantities in a compact analytic form.},
 author = {Celiberto, R and Laricchiuta, A},
 doi = {10.1088/1361-6587/acb79b},
 journal = {Plasma Physics and Controlled Fusion},
 keywords = {},
 number = {3},
 pages = {035022},
 title = {Electron-impact state-to-state cross sections and rate coefficients for X(ν)→B(ν′) excitation of LiH molecule},
 url = {https://app.dimensions.ai/details/publication/pub.1155428824},
 volume = {65},
 year = {2023}
}

