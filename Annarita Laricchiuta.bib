 title = {The role of electron scattering with vibrationally excited nitrogen molecules on non-equilibrium plasma kinetics},
 url = {https://app.dimensions.ai/details/publication/pub.1058084297},
 volume = {20},
 year = {2013}
}

@article{pub.1058092879,
 abstract = {A simple equation describing the formation of plateaux induced by superelastic collisions in the electron energy distribution function (EEDF) of low temperature and afterglow plasmas is derived. The EEDFs predicted from this equation are in good agreement with those obtained from the numerical solution of the full Boltzmann equation in the presence of excited states.},
 author = {D'Ammando, Giuliano and Colonna, Gianpiero and Capitelli, Mario and Laricchiuta, Annarita},
