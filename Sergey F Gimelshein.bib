}

@inproceedings{pub.1139506909,
 abstract = {Hypersonic chemically reacting flow around a wedge in the near‐continuum regime was numerically studied by the DSMC method with the main goal of validation of real gas effect models. The influence of vibration‐dissociation coupling on the results of numerical simulations was analyzed. To this end, two models of chemical reactions were used in the computations, the total collisional energy model and a vibrationally favored model. The numerical results were compared with the experimental data of Hornung and Smith on the shock‐wave stand‐off distance in a hypersonic flow around the wedge. Sensitivity of simulation results to chemical reaction rate constants was also estimated.},
