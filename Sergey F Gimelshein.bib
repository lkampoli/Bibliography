}

@inproceedings{pub.1098547802,
 abstract = {Dependence of the speed of sound in carbon dioxide as a function of the optical lattice wavelength, varied by changing the angle between two intersecting laser beams, is shown numerically with the direct simulation Monte Carlo method. The change in the speed of sound is related the excitation of vibrational degrees of freedom when the optical lattice - gas interaction regime changes from high to low frequency.},
