 abstract = {Numerical analysis has been conducted to assess the feasibility of using radiometer arrays mounted on a near-space vehicle, for wind disturbance compensation. The results indicate the possibility of using radiometric force for that purpose for altitudes of 80 km and smaller, and head winds up to 30 m/s.},
 author = {Gimelshein, Natalia and Gimelshein, Sergey and Ketsdever, Andrew and Young, Marcus},
