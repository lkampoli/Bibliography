@article{pub.1045431428,
 abstract = {In this paper we propose a simple, unstructured mesh generation technique that is capable of handling the large and complex blade motion that is encountered in certain rotating machines, such as vertical axis wind turbines or cycloidal propellers. The technique is characterised by localised re-meshing and interpolation, so as to keep the mesh generation cost and interpolation error as low as possible.},
