 year = {2022}
}

@inproceedings{pub.1144378138,
 abstract = {View Video Presentation: https://doi.org/10.2514/6.2022-0980.vid The first study of the full coupling between the aerothermodynamics, the magnetohydrodynamics, and the plasma sheaths within earth entry flows is here performed. The problem tackled herein is representative of a force-generating MHD patch located between the plenum and the frustum of a capsule entering the earth’s atmosphere at Mach 34. The reactions are obtained from a modified 11- species Park chemical solver. The physical model fully couples the drift-diffusion model for the sheaths to the multispecies Navier-Stokes equations for the plasma flow. The Hall and ion slip effects are taken into consideration within the plasma flow and within the sheaths. The effect of the electrode material on the MHD process is studied. Using thoriated tungsten instead of graphite leads to a fourfold increase in the Lorentz forces and also leads to significantly reduced heat fluxes on the cathode. This is attributed to the much higher electrical conductivity of the thoriated tungsten sheath reducing by orders of magnitude the plasma electrical resistance near the surfaces.},
 author = {Parent, Bernard and Rajendran, Prasanna T. and Moses, Robert W. and Johnston, Christopher O. and Cheatwood, F. M. and Macheret, Sergey O. and Little, Justin},
 booktitle = {AIAA SCITECH 2022 Forum},
 doi = {10.2514/6.2022-0980},
 keywords = {},
 pages = {},
 title = {Effect of Plasma Sheaths on Earth Re-entry MHD Processes},
 url = {https://app.dimensions.ai/details/publication/pub.1144378138},
 year = {2022}
}

@inproceedings{pub.1144418082,
 abstract = {View Video Presentation: https://doi.org/10.2514/6.2022-2396.vid This paper introduces a method for assessing similarity between available shock-layer ra- diation measurements and the radiative heating to a vehicle of interest. This method will be referred to as flowfield property binning (FPB). The challenge in developing this approach is that radiometer measurements from a surface normal line of sight, observed radiation mea- surements of the entire shock-layer from a distance, or measured shock-tube nonequilibrium profiles are notably different geometrically than the radiative flux to a vehicle surface. There- fore, the relative contributions of various regions of the shock-layer to the measurement, and how these relative contributions compare with the radiative heating to a vehicle of interest, are unclear. By tracking the radiation contribution from each flowfield location, and then grouping this contribution based on two local flowfield properties, these geometric differences are removed. This provides a two dimensional group, or bin, distribution that represents the radiation environment in the same format for various types of radiation simulations, such as observed radiation or radiative flux to a vehicle surface. In the limits of optically-thin radiation transport and thermochemical equilibrium flow, the choice of pressure and temperature as the binning flowfield properties is shown to provide a bin distribution that uniquely defines the radiation simulation. This concept is found to be sufficient for establishing similarity be- tween two cases even when the optically-thin and thermochemical equilibrium assumptions are not satisfied. A simple metric of comparison between bin distributions from different cases is provided by the histogram intersection algorithm. This algorithm results in a histogram intersection factor (H) between 0 and 1, which represents the fraction of the radiation that is similar between the two cases. Therefore, if H evaluated between bin distributions for two com- pletely different radiation problems is near 1, then they represent essentially the same radiation problem. This means that they depend on the same weighting of various radiation modeling parameters evaluated at the same pressure and temperature. If one of these cases agrees well with a radiation measurement, then the matching bin distributions provide justification for applying this experimental validation to the other case. Even for H values as low as 0.2, the measurement case could be considered relevant if there are no other useful measurements. This approach allows observed radiation measurements to directly inform radiative heating simulations, which allows NASA’s past and future investments in observed radiation measure- ments to directly inform the radiative heating uncertainty or margins for future vehicle design. This FPB approach is also used to determine shock-tube conditions for informing backshell radiative heating simulations, where the normal shock velocity and Lagrangian time from the shock are used for the binning variables instead of pressure and temperature.},
 author = {Johnston, Christopher O.},
 booktitle = {AIAA SCITECH 2022 Forum},
 doi = {10.2514/6.2022-2396},
 keywords = {},
 pages = {},
 title = {Shock-Layer Radiation Insights Available Through Flowfield-Property Binning},
 url = {https://app.dimensions.ai/details/publication/pub.1144418082},
 year = {2022}
}

@inproceedings{pub.1148850485,
 abstract = {View Video Presentation: https://doi.org/10.2514/6.2022-3799.vid Recent Hayabusa 2 spectral measurements are compared with state-of-the-art shock-layer radiation simulations resulting from the LAURA/HARA code suite. These simulations include coupled ablation, which accounts for the injection of ablation products into the flowfield, and coupled radiation, which accounts for radiative energy loss in the flowfield. The comparison between the simulations and measurements focuses on three atomic nitrogen lines, two atomic oxygen lines, one atomic hydrogen line, and the CN Violet band system. Parametric uncertain- ties are evaluated for these features using a recently developed capability in LAURA/HARA. The resulting parametric uncertainties range from 50 to over 100% for the atomic lines, and 80% for CN Violet. Uncertainties in flowfield kinetics and non-Boltzmann modeling provide the dominant contribution to these uncertainties. For the atomic nitrogen and oxygen lines, comparisons between the nominal simulations and measurements show agreement well within these simulation uncertainty bounds, with the measurements within 20% of the nominal simu- lation over most of the trajectory, and within 10% at peak emission. For the ablation product emission from the 656 nm atomic hydrogen line, the agreement between measurements and simulations is within ±20% for all trajectory points except one outlier. For the ablation product emission from CN Violet, the simulations overpredict the measurements by over a factor of two early in the trajectory, with the comparison improving later in the trajectory. This disagree- ment for CN Violet is consistent with the ≈±80% parametric uncertainty bounds evaluated for the simulations. To utilize the agreement between the atomic line simulations and measure- ments in the assessment of the radiative heating margin for a future flight vehicle, the flowfield property binning (FPB) approach is applied. Using the Mars Sample Return (MSR) Earth Entry System (EES) as an example, the combination of the favorable Hayabusa comparisons and the FPB analysis justify a potential reduction in the radiative heating margin for the exam- ple considered. This direct quantitative link between observed radiation measurements and the radiative heating margin of future flight vehicles demonstrates the value of these observed radiation measurements.},
 author = {Johnston, Christopher O.},
 booktitle = {AIAA AVIATION 2022 Forum},
 doi = {10.2514/6.2022-3799},
 keywords = {},
 pages = {},
 title = {Comparison between Hayabusa 2 Spectral Measurements and Simulations},
 url = {https://app.dimensions.ai/details/publication/pub.1148850485},
 year = {2022}
}

@inproceedings{pub.1148850489,
 abstract = {View Video Presentation: https://doi.org/10.2514/6.2022-3802.vid The SCIFLI Hayabusa2 Airborne Reentry Observation Campaign (SHARC) resulted in the acquisition of spectral data from a reentering capsule-type spacecraft and results are presented here. The Japan Aerospace Exploration Agency (JAXA) launched the Hayabusa2 probe in 2014 to collect samples from a near-earth asteroid and return them to Earth. The Hayabusa2 sample return capsule (SRC) reentered the Earth’s atmosphere over Australia on 05 December 2020 (UTC). The NASA Scientifically Calibrated In-Flight Imagery (SCIFLI) team equipped two aircraft with a total of ten imaging spectrometers. Datasets were successfully collected on all ten imaging spectrometers and, when combined, covered the ultraviolet (UV) to shortwave infrared (SWIR) (300 to 1700nm) spectral range. The spectral signatures include thermal emission from the SRC and shock layer/wake emission from the ionized flowfield. This paper provides a summary of the instrumentation used and the comprehensive calibration process which included pre-mission characterizations, in-flight star calibrations and post-mission calibrations. The measured signatures include traceable uncertainty bounds and the uncertainty analysis is presented. The calibrated data are shown, and interesting spectral features are discussed. Finally, the measured data are compared with the NASA model predictions.},
 author = {Scriven, Gordon R. and Scott, Carey F. and Inman, Jennifer A. and Tanno, Hideyuki and Yamada, Tetsuya and Johnston, Christopher O. and Prabhu, Dinesh K. and Conn, Robert},
 booktitle = {AIAA AVIATION 2022 Forum},
 doi = {10.2514/6.2022-3802},
 keywords = {},
 pages = {},
 title = {Hayabusa2 SHARC Spectro-Radiometric Calibration and Analysis},
 url = {https://app.dimensions.ai/details/publication/pub.1148850489},
 year = {2022}
