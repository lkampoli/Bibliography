@article{pub.1150272548,
 abstract = {The standard approach for simulating hypersonic dust erosion problems using mixed Eulerian–Lagrangian two-phase solvers may be too computationally expensive for routine simulations, and particularly for uncertainty quantification (UQ) analyses. For efficient analyses, a new approach for predicting the surface dust erosion in hypersonic flows with a sparse set of particles is presented and demonstrated on a problem from literature involving the ExoMars Schiaparelli entry vehicle. This new approach, referred to here as the Trajectory Control Volume (TCV) method, is verified against traditional approaches based on Monte Carlo and is shown to require over three orders of magnitude fewer samples to obtain the same results. An example UQ and global nonlinear sensitivity analysis is performed with the new TCV approach. This analysis, which is not feasible with previous approaches, demonstrates the computational efficiency of the approach for UQ problems as well as provides some insight into the driving factors behind dust erosion. UQ and sensitivity results indicate that accurate characterization of material composition and size distribution for dust environments is key to reducing uncertainty in predicted surface recession due to dust impingement.},
 author = {Hinkle, Andrew and Hosder, Serhat and Johnston, Christopher},
 doi = {10.2514/1.a35331},
 journal = {Journal of Spacecraft and Rockets},
 keywords = {},
 number = {6},
 pages = {1-15},
 title = {Efficient Solution of Surface Erosion in Particle-Laden Hypersonic Flows},
 url = {https://app.dimensions.ai/details/publication/pub.1150272548},
 volume = {59},
 year = {2022}
}

