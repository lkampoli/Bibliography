@inproceedings{pub.1144378140,
 abstract = {View Video Presentation: https://doi.org/10.2514/6.2022-0982.vid Air flow around a hypersonic vehicle is subjected to shocks, which can raise the temperature of the surrounding air and create a weakly ionized plasma near the surface. Electron transpiration cooling (ETC) is proposed as a cooling mechanism for hypersonic vehicles to counter the heat flux to the surface from the ambient air. The basic idea is to cover the surface with a low work function material that emits electrons when heated. Hypersonic flow simulations are carried out to study the effectiveness of electron transpiration cooling for cesiated tungsten surfaces. The weakly ionized plasma in the hypersonic boundary layer was assumed to be quasineutral except for the sheath region. In this study, we analyze different analytical models for the sheath layer simulation especially addressing the role of the sheath edge boundary conditions. The simulations are based on the full fluid model of plasma and also include simplified models of the plasma sheath. Appropriate boundary conditions at the sheath edge for the simulation of the multidimensional hypersonic flow near the vehicle surface were formulated.},
 author = {Sahu, Rupali and Tropina, Albina and Andrienko, Daniil and Miles, Richard B.},
 booktitle = {AIAA SCITECH 2022 Forum},
 doi = {10.2514/6.2022-0982},
 keywords = {},
