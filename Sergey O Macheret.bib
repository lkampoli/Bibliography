@article{pub.1107190016,
 abstract = {Weakly ionized plasmas in argon and nitrogen in a parallel-plate electrode configuration at a gas pressure of several Torr were sustained by repetitive nanosecond pulses with the pulse repetition frequency sufficiently high to ensure that the plasma does not fully decay between the pulses. In order to measure the electron number density decay in the afterglow of each pulse, a custom-constructed 58.1 GHz homodyne microwave interferometer was used. Initial analysis of the measured electron density decay indicates that dissociative recombination with molecular and cluster ions was the dominant electron loss mechanism for both gases, and that the electron thermalization was significantly faster than the decay of their density. The plasma with low electron temperature between the pulses could potentially be used to reduce the Johnson–Nyquist thermal noise in plasma antennas.},
 author = {Podolsky, Vladlen and Khomenko, Andrei and Macheret, Sergey},
 doi = {10.1088/1361-6595/aae35e},
 journal = {Plasma Sources Science and Technology},
 keywords = {},
 number = {10},
 pages = {10lt02},
 title = {Time-resolved measurements of electron number density in argon and nitrogen plasmas sustained by high-voltage, high repetition rate, nanosecond pulses},
 url = {https://app.dimensions.ai/details/publication/pub.1107190016},
 volume = {27},
 year = {2018}
}

@inproceedings{pub.1107700896,
 abstract = {Microwave breakdown of gas and formation of plasma can be a destructive effect that limits power handling of highfrequency devices and circuits with small gaps 1, 2. When such plasma is formed, the insulated air gaps are transformed into conductive regions. Although this phenomenon can result in performance degradation or even device failure, it can alternatively be useful for some high power devices such as limiters 3, attenuators 4 and switches. In microwave-induced plasma, the incoming wave is employed for plasma ignition, and hence there is no concern about power consumption. Also, microwave-driven plasma can operate in the α-discharge regime 5 even at atmospheric pressure and thus requires lower operating voltage and power 6 and has much higher stability and much longer lifetime in comparison with dc plasma 3.},
 author = {Semnani, Abbas and Macheret, Sergey O. and Peroulis, Dimitrios},
