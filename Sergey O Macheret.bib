@inproceedings{pub.1142386741,
 abstract = {As any electrical conductor, weakly ionized plasmas can be potentially used in RF antennas, but since plasmas can be turned on and off and their properties can be changed, plasmas could form a basis for reconfigurable antennas and arrays. Modeling of resonant dipole antennas shows that if the plasma conductivity exceeds a certain level, the antenna gain should be within a few dB of similar metallic antennas. Simulation of antenna arrays also demonstrates that cross-coupling between plasma array elements would be considerably lower than that between metallic dipoles, thus giving plasma an advantage.},
