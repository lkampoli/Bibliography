@inproceedings{pub.1093329993,
 abstract = {A weakly ionized plasma region is, in general, a dispersive electromagnetic medium whose properties depend on, among other quantities, the plasma electron number density and gas pressure. Such frequency-dependent cold plasma properties include permittivity, conductivity, and thicknesses of its quasi-neutral and sheath regions. If the excitation frequency is much smaller than the plasma frequency, plasma tends to behave as a good conductor. On the other hand, if the plasma frequency is much smaller than the operating frequency, plasma behaves more like a transparent layer to electromagnetic waves. In the range between these two extremes, plasma is a lossy dielectric medium. It is in the range though that high frequency tunability can also be achieved. In this talk, we will first review the aforementioned regimes for cold plasmas and their electromagnetic properties. Several experimentally-validated plasma-based tunable RF devices such as tunable resonators, variable attenuators and reconfigurable power limiters will be discussed. Advantages and drawbacks of each approach will also be highlighted. Plasma jets and gas discharge tubes are employed as plasma cells in our designs. Reconfiguration is achieved by controlling either the excitation voltage or the discharge current. The presented results demonstrate the potential of this approach as an alternative tuning technique particularly in emerging applications where conventional solid-state techniques are ineffective due to high temperature, high power, or linearity issues.},
 author = {Semnani, Abbas and Macheret, Sergey O. and Peroulis, Dimitrios},
 booktitle = {2016 IEEE International Conference on Plasma Science (ICOPS)},
 doi = {10.1109/plasma.2016.7534348},
 keywords = {},
 pages = {1-1},
 title = {Tunable RF electronics based on low temperature plasma},
 url = {https://app.dimensions.ai/details/publication/pub.1093329993},
 year = {2016}
}

@inproceedings{pub.1093534356,
 abstract = {A plasma-based tuning technique based on a gas discharge tube (GDT) integrated in a resonant structure is introduced in this paper. Specifically, the GDT is implementing a variable capacitor in a lumped LC resonator. The electron number density and consequently the electromagnetic properties of plasma are controlled by varying the magnitude of the excitation field. In this work we also theoretically and experimentally investigate the tunability-RF loss trade-off. The fabricated GDT LC resonator at 305 MHz shows 26% of tunability while the discharge current increases up to 75 mA. The resonator's quality factor varies between 43.7-2.0 for the same range. However, a quality factor above 10 can be maintained if the tuning range is reduced to 4 %. The results reveal that this technique could be a suitable tuning scheme for applications where other technologies fail due to high-power or harsh-environment requirements.},
 author = {Semnani, Abbas and Macheret, Sergey O. and Peroulis, Dimitrios},
 booktitle = {2016 IEEE Radio and Wireless Symposium (RWS)},
 doi = {10.1109/rws.2016.7444372},
 keywords = {},
