@inproceedings{pub.1107708233,
 abstract = {Cost efficient synthesis of graphene, graphitic nanopetals and carbon nanotubes has garnered tremendous interest by virtue of their plethora of applications in engineering and biosciences. Roll-to-roll radio frequency chemical vapor deposition (R2R RFCVD) is one such method of synthesis where free electrons in a capacitively coupled RF (CCRF) plasma transfer energy to gas molecules to generate highly reactive radical precursors. These precursors combine when they come in contact with a substrate that moves between the electrodes, and create a material film on it. The aim of this work is to model the CCRF discharge in the chamber with the ultimate goal of optimizing the deposition process.},
 author = {Shivkumar, Gayathri and Tholeti, Siva Sashank and Macheret, Sergey O. and Alexeenko, Alina A. and Alrefae, Majed A. and Fisher, Timothy S.},
