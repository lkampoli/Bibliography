@article{pub.1018478952,
 abstract = {An accurate determination of the catalytic property of thermal protection materials is crucial to design reusable atmospheric entry vehicles. This property is determined by combining experimental measurements and simulations of the reactive boundary layer near the material surface. The inductively-driven Plasmatron facility at the von Karman Institute for Fluid Dynamics provides a test environment to analyze gas-surface interactions under effective hypersonic conditions. In this study, we develop an uncertainty quantification methodology to rebuild values of the gas enthalpy and material catalytic property from Plasmatron experiments. A non-intrusive spectral projection method is coupled with an in-house boundary-layer solver, to propagate uncertainties and provide error bars on the rebuilt gas enthalpy and material catalytic property, as well as to determine which uncertainties have the largest contribution to the outputs of the experiments. We show that the uncertainties computed with the methodology developed are significantly reduced compared to those determined using a more conservative engineering approach adopted in the analysis of previous experimental campaigns.},
 author = {Sanson, François and Villedieu, Nadège and Panerai, Francesco and Chazot, Olivier and Congedo, Pietro M. and Magin, Thierry E.},
 doi = {10.1016/j.expthermflusci.2016.11.013},
 journal = {Experimental Thermal and Fluid Science},
 keywords = {},
 note = {https://hal.inria.fr/hal-01398173/file/quantification-uncertainty-catalytic-2.pdf},
 number = {},
 pages = {414-423},
 title = {Quantification of uncertainty on the catalytic property of reusable thermal protection materials from high enthalpy experiments},
 url = {https://app.dimensions.ai/details/publication/pub.1018478952},
 volume = {82},
 year = {2017}
}

