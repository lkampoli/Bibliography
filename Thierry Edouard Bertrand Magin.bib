@article{pub.1039729753,
 abstract = {In rotating detonation combustors, the high operational frequency and temperature level (2000–4000 K) offer the potential of direct power extraction through the magnetohydrodynamic effect. In this paper, first of all, electrical conductivity and chemical properties are evaluated with an in-house-built chemical kinetics library: Mutation++. Those properties are subsequently used in a one-dimensional flow solver that is equipped with a magnetohydrodynamic module. A three-dimensional reactive unsteady Reynolds-averaged Navier–Stokes flow solver is used to determine the total amount of magnetohydrodynamic power extracted from the combustor. Finally, the analysis of the detonation combustor is performed, considering unseeded and seeded flows to enhance the electrical conductivity.},
 author = {Braun, James and Saracoglu, Bayindir H. and Magin, Thierry E. and Paniagua, Guillermo},
