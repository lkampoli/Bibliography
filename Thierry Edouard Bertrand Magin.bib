 keywords = {},
 note = {https://dipot.ulb.ac.be/dspace/bitstream/2013/265001/3/N2paper.pdf},
 number = {16},
 pages = {164107},
 title = {Principal component analysis acceleration of rovibrational coarse-grain models for internal energy excitation and dissociation},
 url = {https://app.dimensions.ai/details/publication/pub.1103695696},
 volume = {148},
 year = {2018}
}

