 title = {Self-Consistent State-to-State Kinetic Modeling of CO2 Cold Plasmas: Insights on the Role of Electronically Excited States},
 url = {https://app.dimensions.ai/details/publication/pub.1165739921},
 volume = {44},
 year = {2023}
}

@article{pub.1167051301,
 abstract = {In the era of space exploration, one of the fundamental goals is the investigation of the atmospheric re-entry of a space vehicle, which undergoes tremendous heat load due to the shock waves forming in front of its thermal shield. In the view of heat mitigation, ablative materials are employed for the design of the Thermal Protection System (TPS) of most of the modern vehicles. Such materials essentially modify the nature of the gas in the boundary layer due to the interaction of the solid and gas phases. Thanks to the material consumption, heat load is mitigated at the expense of ablated species which significantly contaminate the boundary layer gas mixture. In this context, this work aims at characterizing the aerothermodynamics environment around a bluff body in the presence of ablation and thermochemical non-equilibrium. Specifically, a five species neutral air mixture (N 2 , O 2 , NO, N, O) is combined with a six ablated species mixture (CO, CN, CO 2 , C, C 2 , C 3 ). Two test cases are considered. Firstly, a subsonic pure nitrogen flow past an ablative sample is simulated, which is used as verification study by direct comparison with numerical results. For this reason, a classic multitemperature (mT) approach is employed to handle non-equilibrium. The second test case is a well-known hypersonic air flow past a spherical body used as a benchmark to assess the influence of ablation. In this case, two different kinetics models are used, namely the classical multitemperature and the more sophisticated State-to-State (StS) approach.},
 author = {Bonelli, Francesco and Ninni, Davide and Pietanza, Lucia Daniela and Colonna, Gianpiero and Helber, Bernd and Magin, Thierry E. and Pascazio, Giuseppe},
 doi = {10.1016/j.compfluid.2023.106161},
 journal = {Computers & Fluids},
 keywords = {},
 number = {},
 pages = {106161},
 title = {Effects of thermochemical non-equilibrium in the boundary layer of an ablative thermal protection system: A state-to-state approach},
 url = {https://app.dimensions.ai/details/publication/pub.1167051301},
 volume = {270},
 year = {2024}
}

@article{pub.1168352658,
 abstract = {The paper collects and discusses the results obtained in the theoretical investigation of cold plasmas by using a state-to-state self-consistent kinetic approach, coupling chemistry and free electron kinetics. Examples are selected, not only to review the most recent advancements made in updating and extending the chemical model, but also to highlight the role played in all these systems by excited states, either vibrational or electronic, in affecting the plasma evolution in the discharge and in the post-discharge phases in different discharge configurations. The response of the kinetic simulation to the accuracy of the dynamical data describing the collisional processes, to the theoretical scheme adopted for the vibrational levels of molecules, and to the inclusion of the relevant dissociation channels, is discussed also in the light of the comparison with experiments for model validation.},
 author = {Capitelli, M and Celiberto, R and Colonna, G and Laricchiuta, A and Pietanza, L D},
 doi = {10.1016/j.fpp.2024.100037},
 journal = {Fundamental Plasma Physics},
 keywords = {},
 note = {https://doi.org/10.1016/j.fpp.2024.100037},
 number = {},
 pages = {100037},
 title = {The self-consistent approach in cold plasma kinetics: From negative ion sources to molecular activation},
