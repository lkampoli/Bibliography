 year = {2012}
}

@inproceedings{pub.1098545993,
 abstract = {A detailed collisional-radiative model (CRM) of a 7-species Jovian plasma composed by H2,H2+,H,H+,He,He+,e− species has been developed by extending a previous model of a pure hydrogen plasma. The CRM is self-consistently coupled with a Boltzmann equation for the non-equilbrium electron energy distribution and a radiative transfer equation for the specific intensity Iν, radiative rate coefficients and radiative energy transfer in one-dimensional slab geometry. In this work the model is used to study a steady shock wave under conditions that are typical of an high speed entry in Jupiter's atmosphere. The model predicts highly non-equilibrium electron and atomic distributions as well as strong reabsorption of atomic radiation in the post-shock region.},
