@article{pub.1045639316,
 abstract = {State-to-state non-equilibrium plasma kinetics is widely used to characterize cold molecular and reentry plasmas. The approach requires a high level of dynamical information, and demands a large effort in the creation of complete databases of state-resolved cross sections and rate coefficients. Recent results, emphasizing the dependence of elementary process probability on both the vibrational and rotational energy content of the H2 molecule, are presented for those channels governing the microscopic collisional dynamics in non-equilibrium plasmas, i.e. electron-impact induced resonant processes, vibrational deactivation and dissociation in atom–diatom collisions and atomic recombination at the surface. Results for H2 plasmas, i.e. negative ion sources for neutral beam injection in fusion reactors, RF parallel-plate reactors for microelectronics, atmospheric discharges and the shock wave formed in the hypersonic entry of vehicles in planetary atmosphere for aerothermodynamics, are discussed.},
 author = {Capitelli, M and Celiberto, R and Colonna, G and D'Ammando, G and De Pascale, O and Diomede, P and Esposito, F and Gorse, C and Laricchiuta, A and Longo, S and Pietanza, L D and Taccogna, F},
 doi = {10.1088/0741-3335/53/12/124007},
 journal = {Plasma Physics and Controlled Fusion},
 keywords = {},
 number = {12},
 pages = {124007},
 title = {Plasma kinetics in molecular plasmas and modeling of reentry plasmas},
 url = {https://app.dimensions.ai/details/publication/pub.1045639316},
 volume = {53},
 year = {2011}
}

