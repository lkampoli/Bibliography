@article{pub.1058055433,
 abstract = {In this paper, we have discussed the effects of electronically excited states of atomic species in affecting the isentropic coefficients of plasmas, focusing on mixtures representing the atmospheres of Jupiter, Mars, and Earth. General behaviors have been rationalized on the basis of simplified approaches. The contribution of the electronically excited states has been evidenced by comparing results obtained considering only the ground state and those obtained using either Fermi or Griem cutoff criteria.},
