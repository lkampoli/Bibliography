 journal = {Acta Astronautica},
 keywords = {},
 number = {},
 pages = {247-258},
 title = {On the influence of non equilibrium in the free stream conditions of high enthalpy oxygen flows around a double-cone},
 url = {https://app.dimensions.ai/details/publication/pub.1150958721},
 volume = {201},
 year = {2022}
}

