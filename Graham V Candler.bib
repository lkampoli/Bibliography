}

@article{pub.1091576605,
 abstract = {This is the final report for efforts made under AFOSR MURI Award No. FA9550-05-1-0341, including summaries of the research effort at UT-IAT, TTU, UMN, and UNO. Efforts at UT-IAT focused on developing methods to improve the integrity of the augmented launcher bore using high-pressure assembly techniques and improved ceramic tiles to provide longer bore life for research at velocities up to 7 km/s. Researchers at TTU have designed a 40-stage, high-efficiency, distributed synchronous launcher to demonstrate operation at high velocities (the goal is 10 km/s) with plasma arcs. Researchers at UMN and UNO have continued to make progress in defining the requirements for the thermal protection system for hypervelocity projectile launches from high altitude at Mach numbers of 25.},
