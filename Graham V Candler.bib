 year = {2013}
}

@article{pub.1091549915,
 abstract = {A computational study of the experiments performed by Lowry et al. at the Arnold Engineering and Development Center (AEDC) has been performed. In these experiments, a RF discharge is used to weakly ionize a volume of air; then a projectile is fired through this plasma. Relative to the conditions without the discharge, the shock standoff distance is observed to increase substantially, and the bow shock becomes flatter. The RF discharge and the resulting thermo-chemical state of the air within the discharge region have been modeled. Based on these conditions, the projectile flow field was simulated to determine if the relaxation of the stored internal energy causes the shock movement. The results obtained indicate that the stored internal energy does not relax fast enough and therefore this effect is not responsible for the observed effects. Two additional mechanisms are considered, and it is found that unsteady flight through thermal non-uniformities is the likely explanation.},
 author = {Candler, Graham V.},
 doi = {10.21236/ada388179},
 journal = {},
 keywords = {},
