@inbook{pub.1004940980,
 abstract = {Abstract The high temperatures encountered at hypersonic speeds trigger complex thermochemical effects in the surrounding medium and have a substantial impact on the vehicle. In addition to the excitation of internal energy modes, chemical reactions such as dissociation and ionization may also occur. The resultant species can then interact with the vehicle surface to produce diverse thermal effects. In this chapter, some of the main concepts are summarized. The notion of nonequilibrium is introduced and related to collisionality. The mechanisms by which chemical reactions occur, the rates at which they proceed, their effect on momentum and energy balance and the coupling between internal energy and chemistry are exposed by deriving the principal conservation equations. Methods of obtaining the equation of state, as well as various transport properties such as viscosity and thermal conductivity are presented. The chapter concludes with an illustrative example of a waverider vehicle traveling at Mach 8.},
 author = {Candler, Graham V.},
 booktitle = {Encyclopedia of Aerospace Engineering},
 doi = {10.1002/9780470686652.eae038},
 keywords = {},
 pages = {},
 publisher = {},
 title = {High‐Temperature Effects in Hypersonic Flight},
 url = {https://app.dimensions.ai/details/publication/pub.1004940980},
 year = {2010}
}

