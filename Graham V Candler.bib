 number = {3},
 pages = {556-569},
 title = {Numerical Simulations of Shock Propagation Under Strong Nonequilibrium Conditions},
 url = {https://app.dimensions.ai/details/publication/pub.1127253824},
 volume = {34},
 year = {2020}
}

