 pages = {369-389},
 title = {Non-equilibrium electron and phonon dynamics in metals under femtosecond laser pulses},
 url = {https://app.dimensions.ai/details/publication/pub.1043926147},
 volume = {45},
 year = {2007}
}

@inbook{pub.1044034931,
 abstract = {Non-equilibrium plasma kinetics plays a key role in understanding chemical reactions carried out under plasma conditions. In turn the coupling between the electron energy distribution function (eedf) of the free electrons and the heavy particle kinetics is the central point of the problem. This point has been several times emphasized by our group1. Nowadays it is considered by numerous researchers who have also added the coupling of these kinetics to the plasmaphysics describing the electrical field seen by discharge electrons. Selfconsistent models have been therefore built up which are very useful in rationalizing the experimental findings. A problem however arises when comparing theoretical and experimental results. The models infact contain a lot of elementary processes, the cross sections of which are in many cases poorly known so that a perfect agreement between experimental and theoretical results could appear suspicious. However selfconsistent models contain a lot of informations useful not only to understand experimental results hut also to be used as predictive tools.},
 author = {Capitelli, M. and Gorse, C. and Longo, S.},
