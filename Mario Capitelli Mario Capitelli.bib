 url = {https://app.dimensions.ai/details/publication/pub.1085110552},
 volume = {5},
 year = {2017}
}

@article{pub.1085847244,
 abstract = {Transport-controlling collision integrals have been computed. within a classical framework, for alkali ions interacting with rare gases and for a fairly large range of temperatures. Ab initio calculated potential curves for these systems have been employed for the first time to obtain cross sections, after having fitted several chosen functional forms to their numerical values. The different ranges of validity for each of the forms, that are among those most generally used in the literature within empirical models, are discussed at length and a set of parameters obtained to yield a continuous, smoothly connected, analytical expression for each curve. Some of the computed [math] values are then compared with the corresponding values that are obtained by using « experimental » potentials.},
