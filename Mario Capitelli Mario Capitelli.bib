 pages = {163-186},
 title = {The influence of atoms and vibrationally excited molecules on the electron relaxation in the postdischarge regime of a weakly ionized hydrogen plasma},
 url = {https://app.dimensions.ai/details/publication/pub.1041892483},
 volume = {70},
 year = {1982}
}

