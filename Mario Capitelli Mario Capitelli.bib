}

@article{pub.1065118515,
 abstract = {We present a theoretical approach to interpreting optical emission spectroscopy measurements for nonequilibrium conditions. In this approach both the fluid dynamics and the kinetics of laser-induced plasma are taken into account, and the results obtained by the numerical model are applied to the spectroscopic observation of the plasma induced by the interaction between a KrF laser and a metallic Ti target. We have generalized the theoretical method to calculate the initial conditions for the plume expansion that show the best agreement with experimental results.},
