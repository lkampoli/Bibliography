 volume = {80},
 year = {1984}
}

@article{pub.1058025330,
 abstract = {We report nonempirical calculations for differential and integral elastic scattering cross sections, absorption cross sections (accounting for electronic inelasticity), and total scattering cross sections for electron–H2 scattering at 10–100 eV. The calculations are based on a complex, energy-dependent effective potential consisting of four terms: A static potential calculated from ab initio extended-basis-set Hartree–Fock (EBSHF) wave functions, a polarization potential calculated from an ab initio EBSHF adiabatic polarization potential modified by the local-kinetic-energy semiclassical polarization model to account for nonadiabatic effects, an exchange potential calculated from the EBSHF static electron density and static potential by the semiclassical exchange approximation, and an imaginary absorption potential calculated from the EBSHF static electron density and static potential by the quasifree scattering model with Pauli blocking. We obtain good agreement with all available experimental data at impact energies 10, 40, and 100 eV. Additional calculations with simplified potentials show that nonadiabatic effects on the real polarization potential and the inclusion of an imaginary absorption potential are both essential to the success of the theory except for the large-angle scattering at the highest energy, where the calculated absorption effect is too strong.},
 author = {Staszewska, Grażyna and Schwenke, David W. and Truhlar, Donald G.},
 doi = {10.1063/1.447310},
 journal = {The Journal of Chemical Physics},
 keywords = {},
 number = {1},
 pages = {335-343},
 title = {Complex optical potential model for electron–molecule scattering, elastic scattering, and rotational excitation of H2 at 10–100 eV},
 url = {https://app.dimensions.ai/details/publication/pub.1058025330},
 volume = {81},
 year = {1984}
}

@article{pub.1058025681,
 abstract = {We have performed scattering calculations for vibrationally inelastic collisions of He with I2. We employed close coupling to treat vibration, the infinite-order-sudden approximation for rotation, and a recently reported potential energy surface based on ab initio calculations. We have calculated state-to-state and rotationally summed differential and integral cross sections for three impact energies. The energy dependence of the integral cross sections agrees reasonably well with the recent experiments of Hall et al.},
 author = {Schwenke, David W. and Truhlar, Donald G.},
 doi = {10.1063/1.447661},
 journal = {The Journal of Chemical Physics},
 keywords = {},
 number = {12},
