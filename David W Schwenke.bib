 title = {Accurate partial resonance widths for collinear reactive collisions},
 url = {https://app.dimensions.ai/details/publication/pub.1058031358},
 volume = {87},
 year = {1987}
}

@article{pub.1058031978,
 abstract = {Theoretical spectroscopic constants are reported for the A′ 5Σ+g and C″ 5Πu states of N2 based on CASSCF/MRCI calculations employing large ANO Gaussian basis sets. Our calculated A′ 5Σ+g potential differs qualitatively from previous calculations in that the inner well is significantly deeper (De ≊3450 cm−1). The potential also has a substantial barrier (≊500 cm−1) to dissociation with a maximum near 4.3 a0, and then a shallow (≊47 cm−1) van der Waals minimum near 6.3 a0. We suggest that it is the v=4 and v=5 levels that are involved in predissociating the a 1Πg and B 3Πg states, as opposed to v=0 and v=1 proposed by Verma. The deeper well in the A′ 5Σ+g state provides considerable support for the theory of Berkowitz, Chupka, and Kistiakowsky who proposed that A′ 5Σ+g is the primary precursor state involved in the yellow Lewis–Rayleigh afterglow of nitrogen. The theoretical De for the C″ 5Πu state is also much larger than previous estimates. The previously unassigned Hermann infrared system (HIR) band positions correspond exceptionally well to our theoretical values for the C″ 5Πu →A′ 5Σ+g transition. This and other considerations lead to a rather convincing assignment of this transition as the HIR system. Einstein coefficients and radiative lifetimes are presented for this transition. Since the natural radiative lifetime of this state is much longer than experimental estimates, the C″ 5Πu state may derive its actual lifetime through interaction with the C 3Πu state.},
 author = {Partridge, Harry and Langhoff, Stephen R. and Bauschlicher, Charles W. and Schwenke, David W.},
 doi = {10.1063/1.453962},
 journal = {The Journal of Chemical Physics},
 keywords = {},
 number = {5},
 pages = {3174-3186},
 title = {Theoretical study of the A ′ 5Σ+ g and C ″ 5Π u states of N2: Implications for the N2 afterglow},
 url = {https://app.dimensions.ai/details/publication/pub.1058031978},
 volume = {88},
 year = {1988}
}

@article{pub.1058032045,
 abstract = {A new method for quantum mechanical calculations of cross sections for molecular energy transfer and chemical reactions is presented, and it is applied to inelastic and reactive collisions of I, H, and D with H2. The method involves the expansion in a square-integrable basis set of the amplitude density due to the difference between the true interaction potential and a distortion potential and the solution of a large set of coupled equations for the basis function coefficients. The transition probabilities, which correspond to integrals over the amplitude density, are related straightforwardly to these coefficients.},
 author = {Zhang, John Z. H. and Kouri, Donald J. and Haug, Kenneth and Schwenke, David W. and Shima, Yaakov and Truhlar, Donald G.},
 doi = {10.1063/1.454029},
 journal = {The Journal of Chemical Physics},
 keywords = {},
