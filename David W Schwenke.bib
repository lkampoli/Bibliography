 journal = {Theoretica chimica acta},
 keywords = {},
 number = {5},
 pages = {381-402},
 title = {A new method for the direct calculation of resonance parameters with application to the quasibound states of the H2X1Σg+ system},
 url = {https://app.dimensions.ai/details/publication/pub.1022703794},
 volume = {74},
 year = {1988}
}

